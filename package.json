{
  "author": {
    "name": "Rogger794",
    "email": "rogger.valverde@uni.pe"
  },
  "license": "MIT",
  "repository": "Rogger794/iam-policies",
  "description": "Identity based policies library",
  "version": "0.0.0-semantic-release",
  "keywords": [
    "iam",
    "policies",
    "iam-policies",
    "roles",
    "permissions"
  ],
  "name": "iam-policies",
  "bugs": {
    "url": "https://github.com/Rogger794/iam-policies/issues"
  },
  "homepage": "https://github.com/Rogger794/iam-policies#readme",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "pretest": "./node_modules/.bin/eslint . --fix",
    "prepublishOnly": "npm run build",
    "test": "jest /tests",
    "test:watch": "jest /tests --watch",
    "clean": "rimraf dist",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "start": "npm-run-all clean build:watch",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "cm": "git cz",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "source": "main.ts",
  "main": "dist/main.js",
  "module": "dist/main.es.js",
  "jsnext:main": "dist/main.es.js",
  "typings": "dist/main.d.ts",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^24.0.22",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "markdownlint": "^0.18.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "yarn pretest",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
